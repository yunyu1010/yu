<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yao.yu.mapper.StudentMapper">

    <select id="getStudentByName" resultType="com.yao.yu.entity.Student">
        select username from student where username = #{username}
    </select>


    <select id="totalItemNumber" resultType="java.lang.Integer">
        select count (*) from student
    </select>





</mapper>
